{
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "indent": [2, 2],
    "semi": [2, "never"],
    "curly": [2, "multi"],
    "require-jsdoc": 0,
    "valid-jsdoc": 0,
    "no-use-before-define": 0,
    "space-before-function-paren": 0,
    "dot-location": 0,
    "block-spacing": 0,
    "object-curly-spacing": 0,
    "no-magic-numbers": 0
  }
}
